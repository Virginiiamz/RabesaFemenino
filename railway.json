{
  "builds": [
    {
      "src": "backend_rabesa/package.json",
      "use": "@railway/node"
    },
    {
      "src": "frontend_rabesa/package.json",
      "use": "@railway/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend"
    },
    {
      "src": "/(.*)",
      "dest": "frontend"
    }
  ]
}